{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "useTabs": false,
  "printWidth": 100,
  "overrides": [
    {
      "files": "*.tsx",
      "options": {
        "printWidth": 80
      }
    }
  ],
  "plugins": ["prettier-plugin-tailwindcss"],
  "tailwindFunctions": ["cx", "twix", "twMerge"]
}
